{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-microsoft-contrib"
  ],
  "rules": {
    "missing-jsdoc": false,
    "export-name": false,
    "no-default-export": false,
    "no-relative-imports": false,
    "no-implicit-dependencies": false,
    "variable-name": false,
    "linebreak-style": false,
    "no-any": false,
    "no-console": false
  }
}
